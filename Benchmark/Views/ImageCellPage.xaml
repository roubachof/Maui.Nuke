<?xml version="1.0" encoding="utf-8" ?>
<profiler:ProfilerPage 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:profiler="clr-namespace:Benchmark.Profiler;assembly=Benchmark"
    x:Class="Benchmark.Views.ImageCellPage"
    Title="ImageCells">
    <ListView ItemsSource="{Binding}" CachingStrategy="RecycleElement">
        <ListView.ItemTemplate>
            <DataTemplate>
                <ImageCell Text="Smooth, baby" ImageSource="{Binding}" />
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</profiler:ProfilerPage>